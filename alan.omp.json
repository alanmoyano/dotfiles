{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "overflow": "break",
      "segments": [
        {
          "background": "p:blue",
          "foreground": "p:wight",
          "leading_diamond": "ÓÇ∂",
          "powerline_symbol": "ÓÇ¥",
          "style": "diamond",
          "template": " {{ .Icon }} ",
          "type": "os"
        },
        {
          "background": "p:pink",
          "foreground": "p:text",
          "powerline_symbol": "ÓÇ¥",
          "properties": {
            "home_icon": "~",
            "style": "full"
          },
          "style": "powerline",
          "template": " \uf07b {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#addb67",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#e4cf6a{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f78c6c{{ end }}",
            "{{ if gt .Ahead 0 }}#C792EA{{ end }}",
            "{{ if gt .Behind 0 }}#c792ea{{ end }}"
          ],
          "foreground": "#011627",
          "powerline_symbol": "ÓÇ¥",
          "properties": {
            "branch_gone_icon": "\uf4ad",
            "branch_icon": "Óú• ",
            "branch_identical_icon": "",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Staging.Changed }} {{ .Staging.String }}{{ end }}{{ if .Working.Changed }} {{ .Working.String }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "p:os",
          "foreground": "p:wight",
          "powerline_symbol": "ÓÇ¥",
          "style": "powerline",
          "template": " \uf120",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "overflow": "break",
      "segments": [
        {
          "background": "p:lavender",
          "background_templates": [
            "{{ if gt .Code 0 }}p:red{{end}}"
          ],
          "foreground": "p:wight",
          "invert_powerline": true,
          "leading_diamond": "ÓÇ∂",
          "powerline_symbol": "ÓÇ∂",
          "properties": {
            "always_enabled": true
          },
          "style": "{{ if .Segments.Executiontime.Ms }}powerline{{ else }}diamond{{ end }}",
          "template": " {{ if gt .Code 0 }}{{ .Code }}{{ else }}\uf00c{{ end }} ",
          "trailing_diamond": "ÓÇ¥",
          "type": "status"
        },
        {
          "background": "p:grey",
          "foreground": "p:wight",
          "invert_powerline": true,
          "powerline_symbol": "ÓÇ∂",
          "properties": {
            "style": "amarillo",
            "threshold": 2
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} \uf252",
          "trailing_diamond": "ÓÇ¥",
          "type": "executiontime"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{ if .Root }}‚ö° {{ end }}{{ .UserName }} ‚ûî üìÅ{{ .Folder }}",
  "final_space": true,
  "palette": {
    "blue": "#8AADF4",
    "grey": "#8B8989",
    "lavender": "#B7BDF8",
    "os": "#ACB0BE",
    "pink": "#F5BDE6",
    "red": "#FF6961",
    "text": "#494D64",
    "wight": "#FFFFFF"
  },
  "version": 2,
  "pepe": "si"
}
