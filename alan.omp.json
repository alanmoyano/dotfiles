{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "overflow": "break",
      "segments": [
        {
          "background": "p:path",
          "foreground": "p:text-dark",
          "leading_diamond": "\ue0b6",
          "properties": {
            "home_icon": "~",
            "mapped_locations": {
              "\\\\wsl.localhost\\Ubuntu\\": "Ubuntu"
            },
            "style": "full"
          },
          "style": "diamond",
          "template": " \uf07b {{ .Path }} ",
          "trailing_diamond": "\ue0b4",
          "type": "path"
        },
        {
          "background": "p:git-nice",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:git-changed{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-conflict{{ end }}",
            "{{ if gt .Ahead 0 }}p:git-remote{{ end }}",
            "{{ if gt .Behind 0 }}p:git-remote{{ end }}"
          ],
          "foreground": "p:text-dark",
          "properties": {
            "branch_gone_icon": "\uf4ad",
            "branch_icon": "\ue725 ",
            "branch_identical_icon": "",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "style": "diamond",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Staging.Changed }} {{ .Staging.String }}{{ end }}{{ if .Working.Changed }} {{ .Working.String }}{{ end }} ",
          "trailing_diamond": "\ue0b4",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "p:status",
          "background_templates": [
            "{{ if gt .Code 0 }}p:red{{end}}"
          ],
          "foreground": "p:text-light",
          "invert_powerline": true,
          "leading_diamond": "\ue0b6",
          "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": " {{ if gt .Code 0 }}{{ .Code }}{{ else }}\uf00c{{ end }} ",
          "trailing_diamond": "\ue0b4",
          "type": "status"
        },
        {
          "background": "p:time",
          "foreground": "p:text-light",
          "invert_powerline": true,
          "properties": {
            "style": "amarillo",
            "threshold": 5
          },
          "style": "diamond",
          "template": " {{ .FormattedMs }} \uf252",
          "trailing_diamond": "\ue0b4",
          "type": "executiontime"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{ if .Root }}‚ö° {{ end }}{{ .UserName }} ‚ûî üìÅ{{ .Folder }}",
  "final_space": true,
  "palette": {
    "git-changed": "#F2E5A5",
    "git-conflict": "#F78C6C",
    "git-nice": "#D7F2A5",
    "git-remote": "#b6a5f2",
    "path": "#ddacd5",
    "red": "#F55C54",
    "status": "#CA9BE9",
    "text-dark": "#011627",
    "text-light": "#FCF6F6",
    "time": "#8E819C"
  },
  "version": 2
}